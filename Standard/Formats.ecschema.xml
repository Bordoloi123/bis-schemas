<?xml version="1.0" encoding="utf-8"?>
<ECSchema schemaName="Formats" alias="f" version="01.00.00" description="Standard Set of Format definitions" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.2">
    <ECSchemaReference name="Units" version="01.00.00" alias="u"/>

    <Format typeName="SignedReal" displayLabel="realSign" type="decimal" precision="6" showSignOption="signAlways" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="ParensReal" displayLabel="realPth" type="decimal" precision="6" showSignOption="negativeParentheses" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="DefaultReal" displayLabel="real" type="decimal" precision="6" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="DefaultRealU" displayLabel="realu" type="decimal" precision="6" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel"/>
    <Format typeName="DefaultRealUNS" type="decimal" precision="6" uomSeparator="" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel"/>

    <Format typeName="SignedFractional" displayLabel="fract" type="fractional" precision="64" showSignOption="signAlways" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="Fractional" displayLabel="fract" type="fractional" precision="64" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="FractionalU" displayLabel="fractu" type="fractional" precision="64" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel"/>

    <Format typeName="Scientific" displayLabel="sci" type="scientific" scientificType="normalized" precision="6" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="SignedScientific" displayLabel="sciSign" type="scientific" scientificType="normalized" precision="6" showSignOption="signAlways" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="ScientificNormal" displayLabel="sciN" type="scientific" scientificType="zeroNormalized" precision="6" formatTraits="keepSingleZero|keepDecimalPoint"/>

    <Format typeName="Station_100" type="station" precision="2" stationOffsetSize="2" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="StationU_100" type="station" precision="2" stationOffsetSize="2" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel"/>
    <Format typeName="StationUNS_100" type="station" precision="2" stationOffsetSize="2" uomSeparator="" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel"/>
    <Format typeName="StationUZ_100" type="station" precision="2" stationOffsetSize="2" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel|trailZeroes"/>
    <Format typeName="Station_1000" type="station" precision="2" stationOffsetSize="3" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="StationU_1000" type="station" precision="2" stationOffsetSize="3" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel"/>

    <Format typeName="Station_100_4" type="station" precision="2" stationOffsetSize="2" minWidth="4" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="StationU_100_4" type="station" precision="2" stationOffsetSize="2" minWidth="4" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel"/>

    <Format typeName="Station_1000_4" type="station" precision="2" stationOffsetSize="3" minWidth="4" formatTraits="keepSingleZero|keepDecimalPoint"/>
    <Format typeName="StationU_1000_4" type="station" precision="2" stationOffsetSize="3" minWidth="4" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel"/>

    <Format typeName="StationZ_100_2" type="station" precision="2" stationOffsetSize="2" minWidth="2" formatTraits="keepSingleZero|keepDecimalPoint|trailZeroes"/>
    <Format typeName="StationZ_1000_3" type="station" precision="2" stationOffsetSize="3" minWidth="3" formatTraits="keepSingleZero|keepDecimalPoint|trailZeroes"/>

    <Format typeName="AngleDMS" displayLabel="dms" type="decimal" precision="4" uomSeparator="" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel">
        <Composite spacer="" >
            <Unit label="&#176;">u:ARC_DEG</Unit>
            <Unit label="'">u:ARC_MINUTE</Unit>
            <Unit label="&quot;">u:ARC_SECOND</Unit>
        </Composite>
    </Format>

    <Format typeName="AngleDM" displayLabel="dm" type="decimal" precision="8" uomSeparator="" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel">
        <Composite spacer="" >
            <Unit label="&#176;">u:ARC_DEG</Unit>
            <Unit label="'">u:ARC_MINUTE</Unit>
        </Composite>
    </Format>

    <Format typeName="AmerMYFI" displayLabel="myfi" type="fractional" precision="4" uomSeparator="" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel">
        <Composite>
            <Unit label="mile(s)">u:MILE</Unit>
            <Unit label="yrd(s)">u:YRD</Unit>
            <Unit label="'">u:FT</Unit>
            <Unit label="&quot;">u:IN</Unit>
        </Composite>
    </Format>

    <Format typeName="AmerFI" displayLabel="fi" type="fractional" precision="8" uomSeparator="" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel">
        <Composite spacer="" >
            <Unit label="'">u:FT</Unit>
            <Unit label="&quot;">u:IN</Unit>
        </Composite>
    </Format>

    <Format typeName="AmerYFI" displayLabel="yfi" type="fractional" precision="8" uomSeparator="" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel">
        <Composite>
            <Unit label="yrd(s)">u:YRD</Unit>
            <Unit label="'">u:FT</Unit>
            <Unit label="&quot;">u:IN</Unit>
        </Composite>
    </Format>

    <Format typeName="HMS" type="decimal" precision="2" formatTraits="keepSingleZero|keepDecimalPoint|showUnitLabel">
        <Composite>
            <Unit label="hour(s)">u:HR</Unit>
            <Unit label="min">u:MIN</Unit>
            <Unit label="sec">u:S</Unit>
        </Composite>
    </Format>
</ECSchema>